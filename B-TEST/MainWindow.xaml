<Window x:Class="B_TEST.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:B_TEST"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtMeno" HorizontalAlignment="Left" Margin="106,109,0,0" TextWrapping="Wrap" Text="Meno" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtPriezvisko" HorizontalAlignment="Left" Margin="106,153,0,0" TextWrapping="Wrap" Text="Priezvisko" VerticalAlignment="Top" Width="120" TextChanged="txtMeno_Copy_TextChanged"/>
        <Button x:Name="btnSend" Content="Send" HorizontalAlignment="Left" Margin="195,217,0,0" VerticalAlignment="Top" Click="btnSend_Click"/>
        <TextBlock x:Name="Pocet" HorizontalAlignment="Left" Margin="117,224,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <DataGrid x:Name="Vypis" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="276,41,0,0"/>

    </Grid>
</Window>
